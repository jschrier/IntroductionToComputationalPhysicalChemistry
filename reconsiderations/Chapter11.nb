Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"coinUpdate", "[", "initialState_List", "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"whichCoin", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "2"}], "}"}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"newValue", "=", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"0", ",", "1"}], "}"}], "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"ReplacePart", "[", 
      RowBox[{"initialState", ",", 
       RowBox[{"whichCoin", "\[Rule]", "newValue"}]}], "]"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"samples", "=", 
    RowBox[{"NestList", "[", 
     RowBox[{"coinUpdate", ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], ",", "100"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"summary", "=", 
   RowBox[{"KeySort", "@", 
    RowBox[{"Counts", "[", "samples", "]"}]}]}], " ", 
  RowBox[{"(*", 
   RowBox[{"merely", " ", "agregate"}], "*)"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
