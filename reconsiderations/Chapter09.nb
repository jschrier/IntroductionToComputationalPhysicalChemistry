Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"HAtomVMC0", "[", 
    RowBox[{"alpha_", ",", "nSamples_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"numerator", "=", "0"}], ",", 
       RowBox[{"denominator", "=", "0"}], ",", "r", ",", "xyz", ",", 
       RowBox[{"Z", "=", "1"}], ",", 
       RowBox[{"el", "=", "0"}], ",", 
       RowBox[{"mu", "=", "1"}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"Do", "[", 
       RowBox[{"(*", 
        RowBox[{
        "loop", " ", "over", " ", "nSamples", " ", "to", " ", "evaluate", " ",
          "integrals"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"xyz", "=", 
          RowBox[{"RandomReal", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
            RowBox[{"{", "3", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"r", "=", 
          RowBox[{"Norm", "[", "xyz", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"numerator", "+=", 
          RowBox[{
           RowBox[{"trialHAtom", "[", 
            RowBox[{"r", ",", "alpha"}], "]"}], "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"-", "1"}], "/", 
               RowBox[{"(", 
                RowBox[{"2", "*", "mu"}], ")"}]}], "*", "\[IndentingNewLine]", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"trialHAtomSecondDeriv", "[", 
                 RowBox[{"r", ",", "alpha"}], "]"}], "+", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"2", "/", "r"}], ")"}], "*", 
                 RowBox[{"trialHAtomFirstDeriv", "[", 
                  RowBox[{"r", ",", "alpha"}], "]"}]}]}], ")"}]}], "+", 
             RowBox[{"el", "*", 
              RowBox[{"(", 
               RowBox[{"el", "+", "1"}], ")"}], "*", 
              RowBox[{
               RowBox[{"trialHAtom", "[", 
                RowBox[{"r", ",", "alpha"}], "]"}], "/", 
               RowBox[{"(", 
                RowBox[{"2", "*", "mu", "*", 
                 SuperscriptBox["r", "2"]}], ")"}]}]}], "-", 
             RowBox[{"Z", "*", 
              RowBox[{
               RowBox[{"trialHAtom", "[", 
                RowBox[{"r", ",", "alpha"}], "]"}], "/", "r"}]}]}], ")"}]}]}],
          ";", 
         RowBox[{"denominator", "+=", 
          SuperscriptBox[
           RowBox[{"trialHAtom", "[", 
            RowBox[{"r", ",", "alpha"}], "]"}], "2"]}], ";"}], 
        "\[IndentingNewLine]", ",", 
        RowBox[{"{", "nSamples", "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"numerator", "/=", "nSamples"}], ";", "\[IndentingNewLine]", 
      RowBox[{"denominator", "/=", "nSamples"}], ";", "\[IndentingNewLine]", 
      RowBox[{"numerator", "/", "denominator"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
