704da06d2572"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"probHAtom", "[", 
   RowBox[{"r_", ",", "alpha_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["alpha", "3"], "/", "Pi"}], ")"}], "*", 
   SuperscriptBox[
    RowBox[{"trialHAtom", "[", 
     RowBox[{"alpha", ",", "r"}], "]"}], "2"]}]}], "\n", 
 RowBox[{
  RowBox[{"KEHAtom", "[", 
   RowBox[{"r_", ",", "alpha_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"alpha", "/", "r"}], ")"}], "-", 
   RowBox[{
    SuperscriptBox["alpha", "2"], "/", "2"}]}]}], "\n", 
 RowBox[{
  RowBox[{"VHAtom", "[", "r_", "]"}], ":=", 
  RowBox[{
   RowBox[{"-", "1"}], "/", "r"}]}]}], "Input",
