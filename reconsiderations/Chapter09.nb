  RowBox[{"areaCircle", "[", "nTotal_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"x", ",", "y", ",", 
      RowBox[{"nInside", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
