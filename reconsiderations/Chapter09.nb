Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"acceptanceFunction", "[", 
    RowBox[{
    "r_", ",", "probabilityFn_", ",", "kineticEnergyFn_", ",", 
     "potentialFn_"}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"probabilityFn", "[", "r", "]"}], ">=", 
      RowBox[{"RandomReal", "[", "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"kineticEnergyFn", "[", "r", "]"}], ",", 
       RowBox[{"potentialFn", "[", "r", "]"}]}], "}"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"HAtomVMC1", "[", 
    RowBox[{"acceptFn_", ",", "nSamples_"}], "]"}], ":=", 
   RowBox[{"With", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"r", "=", " ", 
       RowBox[{"Norm", "/@", 
        RowBox[{"RandomReal", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"nSamples", ",", "3"}], "}"}]}], "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"With", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"values", "=", 
         RowBox[{"Transpose", "@", 
          RowBox[{
           RowBox[{"Map", "[", "acceptFn", "]"}], "@", "r"}]}]}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"With", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"meanKE", " ", "=", " ", 
            RowBox[{"Mean", "[", 
             RowBox[{"values", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"meanPE", "=", 
            RowBox[{"Mean", "[", 
             RowBox[{"values", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "]"}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"acceptedPoints", "=", 
            RowBox[{"Length", "[", "values", "]"}]}]}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"meanKE", ",", "meanPE", ",", 
           RowBox[{"meanKE", "+", "meanPE"}], ",", 
           RowBox[{"acceptedPoints", "/", "nSamples"}]}], "}"}]}], "]"}]}], 
      "]"}]}], "]"}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "define", " ", "a", " ", "pure", " ", "function", " ", "that", " ", 
    "returns", " ", "the", " ", "desired", " ", "value"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"acceptOn", "[", "alpha_", "]"}], ":=", 
   RowBox[{
    RowBox[{"accpetanceFunction", "[", 
     RowBox[{"#", ",", 
      RowBox[{"probHAtom", "[", 
       RowBox[{"#", ",", "alpha"}], "]"}], ",", 
      RowBox[{"KEHAtom", "[", 
       RowBox[{"#", ",", "alpha"}], "]"}], ",", 
      RowBox[{"VHatom", "[", "#", "]"}]}], "]"}], "&"}]}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"usage", " ", "example"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"HatomVMC1", "[", 
  RowBox[{
   RowBox[{"acceptOn", "[", "alpha", "]"}], ",", 
   RowBox[{"10", "^", "4"}]}], "]"}]}], "Input",
