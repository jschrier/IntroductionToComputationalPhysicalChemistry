Cell["\<\
I have found it helpful for students to divide the calculation into a \
numerator and denominator (not for any deep computational reason, but simply \
to assist in understanding ; in keeping with our quest to write everything as \
a pure function, we\[CloseCurlyQuote]ll pass what were previously global \
variables as optional parameters (with default values corresponding to the \
examples in the book):\
\>", "Text",
 CellChangeTimes->{{3.8860080245601053`*^9, 3.8860080405910883`*^9}, {
  3.886011569912739*^9, 3.8860116238419437`*^9}, {3.886011680093728*^9, 
  3.8860117352185183`*^9}, {3.8860119659420967`*^9, 
  3.886012015973611*^9}},ExpressionUUID->"130bf806-f793-4836-bde0-\
89bad4461509"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"numerator", "[", 
    RowBox[{"r_", ",", "alpha_", ",", 
     RowBox[{"Z_", ":", "1"}], ",", 
     RowBox[{"mu_", ":", "1"}], ",", 
     RowBox[{"el_", ":", "0"}]}], "]"}], ":=", 
   RowBox[{
    RowBox[{"trialHAtom", "[", 
     RowBox[{"r", ",", "alpha"}], "]"}], "*", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"-", "1"}], "/", 
        RowBox[{"(", 
         RowBox[{"2", "*", "mu"}], ")"}]}], "*", "\[IndentingNewLine]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"trialHAtomSecondDeriv", "[", 
          RowBox[{"r", ",", "alpha"}], "]"}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"2", "/", "r"}], ")"}], "*", 
          RowBox[{"trialHAtomFirstDeriv", "[", 
           RowBox[{"r", ",", "alpha"}], "]"}]}]}], ")"}]}], "+", 
      RowBox[{"el", "*", 
       RowBox[{"(", 
        RowBox[{"el", "+", "1"}], ")"}], "*", 
       RowBox[{
        RowBox[{"trialHAtom", "[", 
         RowBox[{"r", ",", "alpha"}], "]"}], "/", 
        RowBox[{"(", 
         RowBox[{"2", "*", "mu", "*", 
          SuperscriptBox["r", "2"]}], ")"}]}]}], "-", 
      RowBox[{"Z", "*", 
       RowBox[{
        RowBox[{"trialHAtom", "[", 
         RowBox[{"r", ",", "alpha"}], "]"}], "/", "r"}]}]}], ")"}]}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"denominator", "[", 
    RowBox[{"r_", ",", "alpha_"}], "]"}], ":=", 
   SuperscriptBox[
    RowBox[{"trialHAtom", "[", 
     RowBox[{"r", ",", "alpha"}], "]"}], "2"]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"HAtomVMC0", "[", 
    RowBox[{"alpha_", ",", "nSamples_"}], "]"}], ":=", 
   RowBox[{"With", "[", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "generate", " ", "all", " ", "the", " ", "random", " ", "positions", " ",
       "at", " ", "once"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"r", " ", "=", " ", 
       RowBox[{"Norm", "/@", 
        RowBox[{"RandomReal", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"nSamples", ",", "3"}], "}"}]}], "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"With", "[", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "compute", " ", "the", " ", "means", " ", "in", " ", "one", " ", 
        "shot"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"num", "=", 
          RowBox[{"Mean", "@", 
           RowBox[{
            RowBox[{"Map", "[", 
             RowBox[{
              RowBox[{"numerator", "[", 
               RowBox[{"#", ",", "alpha"}], "]"}], "&"}], "]"}], "@", 
            "r"}]}]}], ",", "\[IndentingNewLine]", 
         RowBox[{"denom", "=", 
          RowBox[{"Mean", "@", 
           RowBox[{
            RowBox[{"Map", "[", 
             RowBox[{
              RowBox[{"denominator", "[", 
               RowBox[{"#", ",", "alpha"}], "]"}], "&"}], "]"}], "@", 
            "r"}]}]}]}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"num", "/", "denom"}]}], "]"}]}], "]"}]}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"perhaps", " ", "a", " ", "bit", " ", "slower"}], ",", " ", 
    RowBox[{"in", " ", "exchange", " ", "for", " ", "simplicity"}]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HAtomVMC0", "[", 
   RowBox[{"1.0", ",", "100000"}], "]"}], "//", "AbsoluteTiming"}]}], "Input",\

