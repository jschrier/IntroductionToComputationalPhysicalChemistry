Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"HAtomVMC1", "[", 
    RowBox[{"alpha_", ",", "nSamples_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"currentSamples", "=", "0"}], ",", 
       RowBox[{"KEaverage", "=", "0"}], ",", 
       RowBox[{"Vaverage", "=", "0"}], ",", "r", ",", "xyz", ",", 
       RowBox[{"nTrials", "=", "0"}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"currentSamples", "<", "nSamples"}], ")"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"xyz", "=", 
          RowBox[{"RandomReal", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
            RowBox[{"{", "3", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"r", "=", 
          RowBox[{"Norm", "[", "xyz", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"nTrials", "++"}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"probHAtom", "[", 
             RowBox[{"r", ",", "alpha"}], "]"}], "\[GreaterEqual]", 
            RowBox[{"RandomReal", "[", "]"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"currentSamples", "++"}], ";", "\[IndentingNewLine]", 
            RowBox[{"KEaverage", "+=", 
             RowBox[{"KEHAtom", "[", 
              RowBox[{"r", ",", "alpha"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"Vaverage", "+=", 
             RowBox[{"VHAtom", "[", "r", "]"}]}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", "]"}],
       ";", "\n", 
      RowBox[{"KEaverage", "/=", "nSamples"}], ";", "\n", 
      RowBox[{"Vaverage", "/=", "nSamples"}], ";", 
      RowBox[{"{", 
       RowBox[{"KEaverage", ",", "Vaverage", ",", 
        RowBox[{"(", 
         RowBox[{"KEaverage", "+", "Vaverage"}], ")"}], ",", 
        RowBox[{"N", "[", 
         RowBox[{"nSamples", "/", "nTrials"}], "]"}]}], "}"}]}]}], "\n", 
    "]"}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
