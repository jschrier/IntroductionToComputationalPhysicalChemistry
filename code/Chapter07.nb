Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"calcDOS", "[", 
     RowBox[{"bandList_", ",", "dE_"}], "]"}], ":=", 
    RowBox[{"Module", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"DOS", "=", 
         RowBox[{"{", "}"}]}], ",", "nStates"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Do", "[", 
        RowBox[{"(*", 
         RowBox[{"loop", " ", "over", " ", "energy", " ", "range"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"nStates", "=", "0"}], ";", "\[IndentingNewLine]", 
          RowBox[{"Do", "[", 
           RowBox[{"(*", 
            RowBox[{
            "loop", " ", "over", " ", "bands", " ", "and", " ", "count", " ", 
             "states", " ", "near", " ", "this", " ", "energy", " ", 
             "value"}], "*)"}], 
           RowBox[{
            RowBox[{
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"energy", "-", 
                 RowBox[{"dE", "/", "2"}]}], "\[LessEqual]", 
                RowBox[{"bandList", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", ",", "j", ",", "2"}], "]"}], "]"}], "<", 
                RowBox[{"energy", "+", 
                 RowBox[{"dE", "/", "2"}]}]}], ",", 
               RowBox[{"nStates", "++"}]}], "]"}], ";"}], 
            "\[IndentingNewLine]", ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", 
              RowBox[{"Length", "[", 
               RowBox[{"bandList", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "]"}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", 
              RowBox[{"Length", "[", "bandList", "]"}]}], "}"}]}], "]"}], ";", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"DOS", ",", 
            RowBox[{"{", 
             RowBox[{"energy", ",", 
              RowBox[{"nStates", "/", "dE"}]}], "}"}]}], "]"}], ";"}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"{", 
          RowBox[{"energy", ",", 
           RowBox[{
            RowBox[{"Min", "[", "bandList", "]"}], "-", "dE"}], ",", 
           RowBox[{
            RowBox[{"Max", "[", "bandList", "]"}], "+", "dE"}], ",", "dE"}], 
          "}"}]}], "]"}], ";", "\[IndentingNewLine]", "DOS"}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], "\n"}]], "Input",
