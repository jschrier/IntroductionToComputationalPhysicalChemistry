Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"effectiveMass", "[", 
    RowBox[{"bandData_", ",", "energy2Hartree_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"EM", "=", "bandData"}], ",", 
       RowBox[{"(*", 
        RowBox[{
        "copy", " ", "the", " ", "band", " ", "information", " ", "to", " ", 
         "get", " ", "the", " ", "k", " ", "values"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"dk2", "=", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"bandData", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "1"}], "]"}], "]"}], "-", 
           RowBox[{"bandData", "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ")"}], "2"]}], ",", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"constant", " ", "delta"}], "-", "k"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"nk", "=", 
        RowBox[{"Length", "[", "bandData", "]"}]}]}], "}"}], ",", 
     RowBox[{"(*", 
      RowBox[{
      "number", " ", "of", " ", "k", " ", "values", " ", "in", " ", "the", 
       " ", "list"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"EM", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "2"}], "]"}], "]"}], "=", 
          RowBox[{"dk2", "/", 
           RowBox[{"(", 
            RowBox[{"energy2Hartree", "*", "\[IndentingNewLine]", 
