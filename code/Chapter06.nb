 RowBox[{
  RowBox[{
   RowBox[{"plotMO", "[", 
    RowBox[{"whichMO_", ",", "xyCoords_", ",", "coeffs_", ",", "nBasis_"}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"graphicsList", "=", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"refArea", "=", 
        RowBox[{"1", "/", 
         RowBox[{"Sqrt", "[", "nBasis", "]"}]}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Do", "[", 
       RowBox[{"(*", 
        RowBox[{"loop", " ", "over", " ", "the", " ", "atoms"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
