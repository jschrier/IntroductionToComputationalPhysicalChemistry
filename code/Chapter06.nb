       RowBox[{"graphicsList", "=", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"refArea", "=", "1"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Do", "[", 
       RowBox[{"(*", 
        RowBox[{"loop", " ", "over", " ", "the", " ", "atoms"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
