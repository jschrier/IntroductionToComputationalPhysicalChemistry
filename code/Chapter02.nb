Cell[BoxData[
 RowBox[{
  RowBox[{"estate2", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Reverse", "[", 
      RowBox[{"Eigenvectors", "[", 
       RowBox[{"hMatr", ",", 
        RowBox[{"-", "2"}]}], "]"}], "]"}], "[", 
