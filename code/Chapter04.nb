Cell["4.2.2 Hydrogen atom", "Subsection",
 CellChangeTimes->{{3.7095137582864*^9, 
  3.709513760319063*^9}},ExpressionUUID->"4d8383d1-3f86-4181-a82f-\
feb2098f9d5c"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"mu", "=", "1"}], ";"}], 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"reduced", " ", "mass"}], ",", 
    RowBox[{"nearly", " ", "one"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"el", "=", "0"}], ";"}], 
  RowBox[{"(*", 
   RowBox[{
   "orbital", " ", "angular", " ", "momentum", " ", "quantum", " ", 
    "number"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Z", "=", "1"}], ";"}], 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"atomic", " ", "number"}], "=", 
    RowBox[{
     RowBox[{"number", " ", "of", " ", "protons", " ", "at", " ", "nucleus"}],
      "=", 
     RowBox[{"nuclear", " ", "charge"}]}]}], "*)"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"trialHAtom", "[", 
     RowBox[{"r_", ",", "alpha_"}], "]"}], ":=", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "alpha"}], "*", "r"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"variationalEnergy", "[", "alpha_", "]"}], ":=", 
   RowBox[{"4", "*", "Pi", "*", 
    RowBox[{
     RowBox[{"NIntegrate", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        SuperscriptBox["r", "2"], "*", 
        RowBox[{"trialHAtom", "[", 
         RowBox[{"r", ",", "alpha"}], "]"}], "*", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"-", "1"}], "/", 
            RowBox[{"(", 
             RowBox[{"2", "*", "mu"}], ")"}]}], "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"D", "[", 
              RowBox[{
               RowBox[{"trialHAtom", "[", 
                RowBox[{"r", ",", "alpha"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"r", ",", "2"}], "}"}]}], "]"}], "+", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"2", "/", "r"}], ")"}], "*", 
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"trialHAtom", "[", 
                 RowBox[{"r", ",", "alpha"}], "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"r", ",", "1"}], "}"}]}], "]"}]}]}], ")"}]}], "+", 
          RowBox[{"el", "*", 
           RowBox[{"(", 
            RowBox[{"el", "+", "1"}], ")"}], "*", 
           RowBox[{
            RowBox[{"trialHAtom", "[", 
             RowBox[{"r", ",", "alpha"}], "]"}], "/", 
            RowBox[{"(", 
             RowBox[{"2", "*", "mu", "*", 
              SuperscriptBox["r", "2"]}], ")"}]}]}], "-", 
          "\[IndentingNewLine]", 
          RowBox[{"Z", "*", 
           RowBox[{
            RowBox[{"trialHAtom", "[", 
             RowBox[{"r", ",", "alpha"}], "]"}], "/", "r"}]}]}], ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"r", ",", 
         SuperscriptBox["10", 
          RowBox[{"-", "6"}]], ",", "1000"}], "}"}]}], "]"}], "/", 
     RowBox[{"(", 
      RowBox[{"4", "*", "Pi", "*", 
       RowBox[{"NIntegrate", "[", 
        RowBox[{
         RowBox[{
          SuperscriptBox["r", "2"], "*", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"trialHAtom", "[", 
             RowBox[{"r", ",", "alpha"}], "]"}], ")"}], "2"]}], ",", 
         RowBox[{"{", 
          RowBox[{"r", ",", 
           SuperscriptBox["10", 
            RowBox[{"-", "6"}]], ",", "1000"}], "}"}]}], "]"}]}], ")"}]}]}]}],
   ";"}]}], "Input",
