Cell["Gaussian orbitals", "Subsubsection",
 CellChangeTimes->{
  3.709513979473955*^9},ExpressionUUID->"3360b3dd-8cb9-4e9b-a5d4-\
5557f9bec163"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"mu", "=", "1"}], ";", 
   RowBox[{"el", "=", "0"}], ";", 
   RowBox[{"Z", "=", "1"}], ";"}], 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"set", " ", "problem", " ", "parameters"}], ",", 
    RowBox[{"in", " ", "atomic", " ", "units"}]}], "*)"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"trialHAtomGaussian", "[", 
     RowBox[{"r_", ",", "alpha_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"2", "*", 
        RowBox[{"alpha", "/", "Pi"}]}], ")"}], "^", 
      RowBox[{"(", 
       RowBox[{"3", "/", "4"}], ")"}]}], "*", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "alpha"}], "*", 
       RowBox[{"r", "^", "2"}]}], "]"}]}]}], ";"}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"variationalEnergy", "[", "alpha_", "]"}], ":=", 
    RowBox[{"4", "*", "Pi", "*", 
     RowBox[{"NIntegrate", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        SuperscriptBox["r", "2"], "*", 
        RowBox[{"trialHAtomGaussian", "[", 
         RowBox[{"r", ",", "alpha"}], "]"}], "*", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"-", "1"}], "/", 
            RowBox[{"(", 
             RowBox[{"2", "*", "mu"}], ")"}]}], "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"D", "[", 
              RowBox[{
               RowBox[{"trialHAtomGaussian", "[", 
                RowBox[{"r", ",", "alpha"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"r", ",", "2"}], "}"}]}], "]"}], "+", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"2", "/", "r"}], ")"}], "*", 
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"trialHAtomGaussian", "[", 
                 RowBox[{"r", ",", "alpha"}], "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"r", ",", "1"}], "}"}]}], "]"}]}]}], ")"}]}], "+", 
          RowBox[{"el", "*", 
           RowBox[{"(", 
            RowBox[{"el", "+", "1"}], ")"}], "*", 
           RowBox[{
            RowBox[{"trialHAtomGaussian", "[", 
             RowBox[{"r", ",", "alpha"}], "]"}], "/", 
            RowBox[{"(", 
             RowBox[{"2", "*", "mu", "*", 
              SuperscriptBox["r", "2"]}], ")"}]}]}], "-", 
          RowBox[{"Z", "*", 
           RowBox[{
            RowBox[{"trialHAtomGaussian", "[", 
             RowBox[{"r", ",", "alpha"}], "]"}], "/", "r"}]}]}], ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"r", ",", 
         SuperscriptBox["10", 
          RowBox[{"-", "6"}]], ",", "1000"}], "}"}]}], "]"}]}]}], ";"}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{"soln", "=", 
  RowBox[{
   RowBox[{"NMinimize", "[", 
    RowBox[{
     RowBox[{"variationalEnergy", "[", "alpha", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"alpha", ",", "0.1", ",", "0.5"}], "}"}]}], "]"}], "//", 
   "Quiet"}]}]}], "Input",
